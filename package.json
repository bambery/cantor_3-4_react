{
  "name": "cantor_3-4_react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bambery.github.io/cantor_3-4_react",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "npx serve -s build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:all": "react-scripts test --verbose --watchAll",
    "test:debug": "NODE_ENV=test react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --collectCoverage --watchAll",
    "lint": "eslint .",
    "check": "eslint . && react-scripts test --watchAll=false --detectOpenHandles"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "gh-pages": "^4.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "./src/App.js",
      "./src/index.js",
      "./src/shared/errors.js"
    ]
  }
}
